{
    "name": "eskafactory/laravel-generator",
    "type": "library",
    "description": "EskaFactory Laravel Generator (fork of InfyOm Laravel Generator)",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Eska Factory",
        "email": "contact@eskafactory.io"
    }],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.35",
        "illuminate/console": "^11.35",
        "laracasts/flash": "^3.2.3",
        "symfony/var-exporter": "^7.2.0",
        "spatie/laravel-html": "^3.11",
        "doctrine/dbal": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.1",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^9.7.0",
        "pestphp/pest": "^3.7.1",
        "pestphp/pest-plugin-laravel": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    },
    "funding": [{
        "type": "opencollective",
        "url": "https://opencollective.com/infyomlabs"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
